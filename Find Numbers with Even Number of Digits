class Solution {
    public int findNumbers(int[] nums) {
        int count = 0;
        String numStr ;
        for(int i = 0 ; i<nums.length;i++){
            numStr = String.valueOf(nums[i]);
            if (numStr.length() % 2 == 0){
                count+=1;
            }
                
        }
        
        return count ;
    }
}
